{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "L2 transfer on mainnet",
      "runtimeExecutable": "tsx",
      "program": "${workspaceFolder}/examples/L2TONTransfer/main.ts",
      "args": ["${workspaceFolder}/examples/L2TONTransfer/input4.json"],
      "cwd": "${workspaceFolder}",
      "skipFiles": ["<node_internals>/**"],
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "tokamak-ch-tx (debug, file inputs)",
      "runtimeExecutable": "tsx",
      "program": "${workspaceFolder}/src/interface/cli/index.ts",
      "args": [
        "tokamak-ch-tx",
        "--previous-state",
        "${workspaceFolder}/examples/L2StateChannel/previous_state_snapshot.json",
        "--transaction",
        "",
        "--block-info",
        "${workspaceFolder}/examples/L2StateChannel/block_info.json",
        "--contract-code",
        "${workspaceFolder}/examples/L2StateChannel/contract_code.json"
      ],
      "cwd": "${workspaceFolder}",
      "skipFiles": ["<node_internals>/**"],
      "console": "integratedTerminal"
    }
  ]
}
