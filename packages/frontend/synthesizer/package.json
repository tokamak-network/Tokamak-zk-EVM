{
  "name": "@tokamak-zk-evm/synthesizer",
  "version": "0.0.10",
  "description": "Tokamak zk-EVM Synthesizer - Interprets an Ethereum transaction as a combination of the library subcircuits.",
  "keywords": [
    "ethereum",
    "zk-EVM",
    "zero-knowledge",
    "zk-SNARK",
    "tokamak",
    "layer2",
    "tokamak-network"
  ],
  "homepage": "https://github.com/tokamak-network/tokamak-zk-evm/tree/main/packages/frontend/synthesizer#readme",
  "bugs": {
    "url": "https://github.com/tokamak-network/tokamak-zk-evm/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tokamak-network/tokamak-zk-evm.git"
  },
  "license": "MIT OR Apache-2.0",
  "author": "Tokamak Network",
  "contributors": [
    "JehyukJang <https://github.com/JehyukJang>",
    "Ale Son <ale@tokamak.network>"
  ],
  "type": "module",
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "bin": {
    "synthesizer": "./dist/cjs/cli/index.js"
  },
  "files": [
    "dist",
    "src",
    "LICENSE-MIT",
    "LICENSE-APACHE",
    "NOTICE"
  ],
  "scripts": {
    "build": "../../../config/monorepo-js/cli/ts-build.sh",
    "clean": "../../../config/monorepo-js//cli/clean-package.sh",
    "cli": "tsx src/cli/index.ts",
    "coverage": "DEBUG=ethjs npx vitest run --coverage.enabled --coverage.reporter=lcov",
    "coverage:test": "npm run test:node && cd ../vm && npm run tester -- --state",
    "docs:build": "typedoc --options typedoc.cjs",
    "examples": "tsx ../../scripts/examples-runner.ts -- evm",
    "examples:build": "npx embedme README.md",
    "formatTest": "node ./scripts/formatTest",
    "lint": "../../../config/monorepo-js//cli/lint.sh",
    "lint:diff": "../../../config/monorepo-js//cli/lint-diff.sh",
    "lint:fix": "../../../config/monorepo-js//cli/lint-fix.sh",
    "profiling": "0x ./benchmarks/run.js profiling",
    "synthesize": "tsx src/cli/index.ts synthesize",
    "synthesizer": "tsx src/cli/index.ts run",
    "test": "npm run test:node && npm run test:browser",
    "test:browser": "npx vitest run --config=./vitest.config.browser.mts",
    "test:node": "npx vitest run",
    "test:tokamak": "npx vitest run test/tokamak",
    "test:synthesizer": "npx vitest run test/**",
    "test:state-chain": "tsx examples/L2StateChannel/test-state-chain.ts",
    "test:bidirectional": "tsx examples/L2StateChannel/test-bidirectional-transfer.ts",
    "test:calldata": "tsx examples/L2StateChannel/test-calldata-state-chain.ts",
    "test:full-flow": "tsx examples/L2StateChannel/test-full-state-channel-flow.ts",
    "test:sepolia": "tsx examples/L2StateChannel/test-sepolia-state-channel.ts",
    "test:single": "tsx examples/L2StateChannel/test-single-proof.ts",
    "test:onchain": "tsx examples/L2StateChannel/onchain-channel-simulation.ts",
    "tsc": "../../../config/monorepo-js//cli/ts-compile.sh"
  },
  "dependencies": {
    "@ethereumjs/block": "^10.0.0",
    "@ethereumjs/common": "^10.0.0",
    "@ethereumjs/evm": "^10.0.0",
    "@ethereumjs/rlp": "^10.0.0",
    "@ethereumjs/statemanager": "^10.0.0",
    "@ethereumjs/tx": "^10.0.0",
    "@ethereumjs/util": "^10.0.0",
    "@ethereumjs/vm": "^10.0.0",
    "@js-sdsl/ordered-map": "^4.4.2",
    "@noble/curves": "^1.9.6",
    "@tokamak-zk-evm/qap-compiler": "^0.0.2",
    "@types/debug": "^4.1.9",
    "@vitest/coverage-v8": "^2.1.8",
    "@zk-kit/imt": "2.0.0-beta.8",
    "app-root-path": "^3.1.0",
    "commander": "^12.0.0",
    "debug": "^4.3.3",
    "dotenv": "^17.2.0",
    "ethereum-cryptography": "^3.0.0",
    "ethers": "^6.14.3",
    "eventemitter3": "^5.0.1",
    "poseidon-bls12381": "^1.0.2",
    "verkle-cryptography-wasm": "^0.4.8",
    "vitest": "^2.1.8"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@paulmillr/trusted-setups": "^0.1.2",
    "@types/benchmark": "^1.0.33",
    "@types/core-js": "^2.5.0",
    "@types/minimist": "^1.2.2",
    "@types/node": "^22.15.14",
    "@types/node-dir": "^0.0.34",
    "@types/rollup-plugin-visualizer": "^4.2.4",
    "@vercel/ncc": "^0.38.3",
    "babel-loader": "^10.0.0",
    "benchmark": "^2.1.4",
    "esbuild": "^0.25.8",
    "level": "^8.0.0",
    "mcl-wasm": "^1.5.0",
    "memory-level": "^1.0.0",
    "micro-eth-signer": "^0.11.0",
    "minimist": "^1.2.5",
    "nexe": "^5.0.0-beta.4",
    "node-dir": "^0.1.17",
    "pkg": "^5.8.1",
    "prettier": "^3.5.3",
    "rollup-plugin-visualizer": "^5.12.0",
    "rustbn-wasm": "^0.4.0",
    "solc": "^0.8.1",
    "split": "^1.0.1",
    "ts-loader": "^9.5.2",
    "typescript": "^4.7.2",
    "webpack": "^5.101.0",
    "webpack-cli": "^6.0.1"
  },
  "engines": {
    "node": ">=18"
  },
  "resolutions": {
    "@synthesizer-libs/util": "0.0.3"
  },
  "pkg": {
    "assets": [
      "node_modules/**/*",
      "qap-compiler/**/*"
    ],
    "scripts": [
      "dist/cjs/**/*.js"
    ],
    "targets": [
      "node18-linux-x64",
      "node18-macos-x64",
      "node18-macos-arm64",
      "node18-win-x64"
    ],
    "outputPath": "bin"
  }
}
